<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}.: MGC24.com - MaxGamedCloud - {{ 'Home Page'|trans }} :.{% endblock %}</title>
        <meta http-equiv="content-language" content="{% block site_language %}en{% endblock %}" />
        <meta name="description" content="{% block site_description %}opis{% endblock %}" />
        <meta name="keywords" content="{% block site_keywords %}opis{% endblock %}" />
        {% stylesheets 
            'css/main.css'
            'css/msdropdown/dd.css'
            'css//mgcbutton/style.css'
            'css/home/style.css'
            'css/portal/gameServers.css'
            'css/portal/news.css'
            'css/client/login.css'
            'css/promo.css'
            'css/client/client.css'
        filter='?yui_js' 
        output='css/main_compiled.css' %}
            <link rel="stylesheet" href="{{ asset_url }}" />
        {% endstylesheets %}
        {% block stylesheets %}{% endblock %}
        
        {% javascripts 
	        'js/jquery-2.0.3.min.js'
	        'js/plugins/jquery.corner.js'
	        'js/plugins/jquery.dd.js'
	        'js/jquery.button.js'
	        'js/application.js' 
	        'js/promo.js' 
        filter='?yui_js' 
        output='js/main_compiled.js' %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        {% block headscripts %}{% endblock %}
        
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.png') }}" />
    </head>
    <body>
        {% for flashMessage in app.session.flashbag.get('notice') %}
            <section class="flash">
                {{ flashMessage|raw }}
            </section>
        {% endfor %}
		<div id="fb-root"></div>
		<script>(function(d, s, id) {
		  var js, fjs = d.getElementsByTagName(s)[0];
		  if (d.getElementById(id)) return;
		  js = d.createElement(s); js.id = id;
		  js.src = "//connect.facebook.net/pl_PL/all.js#xfbml=1&appId=180452908682188";
		  fjs.parentNode.insertBefore(js, fjs);
		}(document, 'script', 'facebook-jssdk'));</script>
		{% block body %}{% endblock %}
        {% include '::footer.html.twig' %}
        {% block javascripts %}{% endblock %}
    </body>
</html>